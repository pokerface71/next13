		"prepare": "husky install",
		"test": "yarn lint"
	},
	"husky": {
		"hooks": {
			"pre-commit": "eslint \"**/*.{ts,tsx,js,jsx}\" && npx stylelint --allow-empty-input  --custom-syntax postcss-scss \"**/*.scss\""
		}
	},
