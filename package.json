{
	"name": "baaz",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "yarn lint && next dev -p 8586",
		"build": "next build",
		"start": "next start -p 4000",
		"lint": "eslint \"**/*.{ts,tsx,js,jsx}\" && npx stylelint --allow-empty-input --custom-syntax postcss-scss \"**/*.scss\"",
